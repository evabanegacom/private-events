<p id="notice"><%= notice %></p>

<h1>Eventsusers</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @eventsusers.each do |eventsuser| %>
      <tr>
        <td><%= link_to 'Show', eventsuser %></td>
        <td><%= link_to 'Edit', edit_eventsuser_path(eventsuser) %></td>
        <td><%= link_to 'Destroy', eventsuser, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

